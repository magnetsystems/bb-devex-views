<pre id="sample-distance-android" class="hidden">MagnetMobileClient magnetMobileClient = MagnetMobileClient.getInstance(this.getApplicationContext());
// Create the controller instance
RestControllerFactory restControllerFactory = new RestControllerFactory(magnetMobileClient);
RestController restController = restControllerFactory.obtainInstance();

// Call the controller
Call&lt;GoogleDistanceResult&gt; callResponse = restController.getDistance(
    "435 Tasso Street Palo Alto CA",
    "Embarcadero Street San Francisco CA",
    "false",
    "driving",
    "en",
    "imperial",
    null);
GoogleDistanceResult googleDistanceResult = callResponse.get();</pre>
<pre id="sample-distance-ios" class="hidden">// Create an instance
RestController *controller = [[RestController alloc] init];

// Call the controller
[controller getDistance:@"435 Tasso Street Palo Alto CA"
   destinations:@"Embarcadero Street San Francisco CA"
         sensor:@"false"
           mode:@"driving"
       language:@"en"
         sensor:@"true"
          units:@"imperial"
        success:^(TimeZoneResult *response) {
            // NSLog(@"response is %@", response);
        }
        failure:^(NSError *error) {
            // NSLog(@"error is %@", error);
        }];</pre>
<pre id="sample-distance-js" class="hidden">var controller = new MagnetJS.Controllers.RestController();

var requestData = {
    origins : "435 Tasso Street Palo Alto CA",
    destinations : "Embarcadero Street San Francisco CA",
    sensor : "false",
    mode : "driving",
    language : "en",
    units : "imperial"
}

controller.googleDistance(requestData, {
    success : function(responseData, details){
        // do something with response data
    },
    error : function(error, details){
        // handle errors
    }
});</pre>
<pre id="sample-distance-curl" class="hidden">curl -X GET --header "Content-Type: application/json"  "http://maps.googleapis.com/maps/api/distancematrix/json?origins=435+Tasso+Street+Palo+Alto+CA&destinations=Embarcadero+Street+San+Francisco+CA&sensor=false&mode=driving&language=en&units=imperial"</pre>

