<!doctype html>
<html class="no-js fuelux" lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Magnet Developers {{=title}}</title>
    <meta name="description" content="Generate mobile native code for REST services.">
    <meta name="keywords" content="REST API,Data binding,Type safe,Mobile code generation,iOS tool REST,Android tool REST,Javascript tool REST,mobile REST API,Xcode plugin,Android Studio plugin,IntelliJ IDEA plugin">
    <link rel="stylesheet" href="/stylesheets/bootstrap3.min.css" />
    <link rel="stylesheet" href="/stylesheets/font-awesome.min.css" />
    <link rel="stylesheet" href="/stylesheets/icomoon.css" />
    <link href='//fonts.googleapis.com/css?family=Roboto:400,500,700,900,300,100' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="/stylesheets/style.css" />
    <link rel="shortcut icon" href="/img/site/favicon.ico" type="image/x-icon" />
    <link rel="apple-touch-icon" href="/img/site/apple-touch-icon.png">
</head>

<body id="{{- activePage }}-body">

<div id="head">
    <div id="head-logo">
        <a href="/"><img src="/img/site/logo_magnet_dark.png" /> <p>
            {{ if(view == 'developer'){ }}
                Developer
            {{ }else if(view == 'factory'){ }}
                Factory
            {{ } }}
        </p></a>
    </div>
    {{ if(view == 'factory'){ }}
    {{ if(typeof sessionUser != 'undefined'){ }}
    <div id="head-controls">
        <!--<a href="#" id="invite-others"><span class="glyphicon glyphicon-plus"></span><span class="icon-user icon-white"></span>span></a>-->
        <a href="#" id="user-panel-toggle"><span class="glyphicon glyphicon-chevron-down"></span> <span id="username-placeholder">{{= sessionUser.email }}</span></a>
    </div>
    {{ }else{ }}
    <div id="head-controls">
        <a href="/account/registration">Register</a>
        <a href="#" class="show-login-popup">Sign In</a>
    </div>
    {{ } }}
    {{ } }}
</div>

{{ if(typeof sessionUser != 'undefined'){ }}
<div id="user-panel" style="display:none">
    <div class="col">
        <span class="user-username">{{= sessionUser.email }}</span><br />
        <span class="user-company">{{= sessionUser.companyName }}</span>
    </div>
    <div class="col2">
        <a href="/profile/">Edit Profile</a>
    </div>
    <div class="btns">
        <a class="btn-logout btn btn-primary">Log Out</a>
    </div>
</div>
{{ } }}

<nav id="navigation" class="navbar navbar-default" role="navigation">
    <div class="container-fluid">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed text-left" data-toggle="collapse" data-target="#nav">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
        </div>
        <div class="collapse navbar-collapse" id="nav">
            <ul class="nav navbar-nav">
                {{ if(view == 'developer'){ }}
                <li class="{{- activePage == 'rest2mobile' ? 'active' : ''}}"><a href="/rest2mobile">rest2mobile</a></li>
                <li class="divider"></li>
                <li class="{{- activePage == 'messaging' ? 'active' : ''}}"><a href="/messaging">Mobile Messaging</a></li>
                <li class="divider"></li>
                <li class="{{- activePage == 'persistence' ? 'active' : ''}}"><a href="/persistence">Entity Modeler</a></li>
                {{ }else{ }}
                <li class="{{- activePage == 'resources' ? 'active' : ''}}"><a href="/resources">Resources</a></li>
                <li class="divider"></li>
                <li class="{{- activePage == 'get-started' ? 'active' : ''}}"><a href="/get-started">Get Started</a></li>
                <li class="divider"></li>
                <li class="{{- activePage == 'docs' ? 'active' : ''}}"><a href="/docs">Documentation</a></li>
                <li class="divider"></li>
                <li class="{{- activePage == 'support' ? 'active' : ''}}"><a href="/support">Support</a></li>
                {{ } }}
            </ul>
            <div class="nav navbar-nav navbar-right">
                <a href="//www.magnet.com/dev-blog">Developer's Blog </a>
            </div>
        </div>
    </div>
</nav>

{{-body}}

<div id="footer" class="clearfix">
    {{ if(view == 'developer'){ }}
    <div class="row-fluid">
        <div class="col-sm-6">
            <p>Copyright &copy; Magnet Systems, Inc. 2014 | <a href="//www.magnet.com/resources/privacy_policy.html">Privacy Policy</a></p>
        </div>
        <div class="col-sm-6">
            <div class="top-links lftflot" style="margin-left:60px"></div><div class="socailfollow top-co-inf rgtflot"><a href="https://www.facebook.com/MagnetSys" class="facebook"><i class="icomoon-facebook"></i></a><a href="https://twitter.com/@magnet_systems" class="twitter"><i class="icomoon-twitter"></i></a><a href="https://www.youtube.com/channel/UC09BNsXO7RXmI8bHHlZpZ8g" class="youtube"><i class="icomoon-youtube"></i></a><a href="https://plus.google.com/100758321520525101306/posts" class="google"><i class="icomoon-google"></i></a><a href="https://www.linkedin.com/company/1327301" class="linkedin"><i class="icomoon-linkedin"></i></a></div>
        </div>
    </div>
    {{ }else if(view == 'factory'){ }}
    <div class="row-fluid">
        <div class="col-sm-12">
            Copyright &copy; Magnet Systems, Inc. 2014 | <a href="//www.magnet.com/resources/privacy_policy.html">Privacy Policy</a>
        </div>
    </div>
    {{ } }}
</div>

<div id="leave-feedback-container">
    <a id="feedback-btn" href="">Leave Feedback</a>
    <div id="feedback-content">
        <p>We welcome any comments or questions.</p>
        <select id="feedback-type-field" class="form-control">
            <option value="Comment">Comment</option>
            <option value="Question">Question</option>
        </select>
        <input id="feedback-name" class="form-control" type="text" placeholder="Name" name="name">
        <input id="feedback-subject" class="form-control" type="text" placeholder="Subject" name="email">
        <input id="feedback-email" class="form-control" type="text" placeholder="Email Address" name="email">
        <textarea id="feedback-message" class="form-control" placeholder="Message"></textarea>
        <button id="submit-feedback-btn" class="btn btn-primary">Submit Feedback</button>
        <img src="/img/site/ajax-loader-sm.gif" style="display:none" />
    </div>
    <div id="feedback-complete" style="display:none">
        <h4>Feedback Has Been<br />Submitted</h4>
        <p>Thank you for submitting your feedback or question. We will respond shortly.</p>
    </div>
    <div id="feedback-error" style="display:none">
        <h4>Error Submitting<br />Feedback</h4>
        <p>There was an error submitting your feedback. Please try again later.</p>
    </div>
</div>

<div id="general-alert" class="modal fade" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title"></h4>
            </div>
            <div class="modal-body"></div>
            <div class="modal-footer">
                <button data-dismiss="modal" type="button" class="btn btn-primary">Ok</button>
            </div>
        </div>
    </div>
</div>

<div id="confirm-alert" class="modal fade" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title"></h4>
            </div>
            <div class="modal-body"></div>
            <div class="modal-footer">
                <button type="button" class="submit btn btn-primary">Yes</button>
                <button data-dismiss="modal" type="button" class="btn btn-default">No</button>
            </div>
        </div>
    </div>
</div>

<div id="error-alert" class="modal fade" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title"></h4>
            </div>
            <div class="modal-body"></div>
            <div class="modal-footer">
                <button data-dismiss="modal" type="button" class="btn btn-primary">Ok</button>
            </div>
        </div>
    </div>
</div>

<div id="watch-video-modal" class="modal fade" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title"></h4>
            </div>
            <div id="watch-video-container" class="modal-body"></div>
        </div>
    </div>
</div>

{{ if(view == 'developer'){ }}
<div id="download-plugin-modal" class="modal fade" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title">Join the Developer Program</h4>
            </div>
            <div class="modal-body">
                <h4>Downloading Plugin...</h4>
                <p>Stay in touch with our program and receive the latest updates.</p>
                <div class="form-group">
                    <label>Email Address</label>
                    <input type="text" class="form-control" name="email" placeholder="your email address" />
                </div>
            </div>
            <div class="modal-footer">
                <button data-dismiss="modal" type="button" class="btn btn-primary">Skip</button>
                <button type="button" class="btn btn-primary newsletter-signup-btn">Submit</button>
            </div>
        </div>
    </div>
</div>
{{ } }}

{{ if(typeof sessionUser == 'undefined'){ }}
<div id="login-modal" class="modal fade" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h3>Sign In</h3>
                <strong class="hidden">This resource requires registration to access.</strong>
                <p>Please Sign In below, or click on <a href="/account/registration">Register</a> to register for a new account.<br />
                    Click on <a href="/account/reset-password">Reset Your Password</a> if you don't remember your password.</p>
            </div>
            <div class="modal-body">
                <div class="modal_errors" style="display:none">
                    <div class="alert alert-danger" role="alert">
                        <strong></strong> <span></span>
                    </div>
                </div>
                <div class="form-horizontal">
                    <div class="form-group">
                        <label class="col-sm-3 control-label" for="name">Email Address</label>
                        <div class="col-sm-8">
                            <input class="form-control" type="text" name="name" placeholder="Email Address">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label" for="password">Password</label>
                        <div class="col-sm-8">
                            <input class="form-control" type="password" name="password" placeholder="Password">
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button data-dismiss="modal" type="button" class="btn btn-default">Close</button>
                <button id="sign-in-btn" type="button" class="btn btn-primary">Sign In</button>
            </div>
        </div>
    </div>
</div>
{{ } }}
<iframe id="downloader" style="display:none"></iframe>

