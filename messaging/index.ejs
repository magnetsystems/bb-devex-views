<div class="clearfix text-center general-bgk">
    <h3>Try it out</h3>
    <div id="messaging-demo">
        <div class="row-fluid clearfix">
            <div class="col-sm-8">
                <div id="messaging-form">
                    <div class="form-group">
                        <select class="form-control" name="message-type">
                            <option value="ping-android" selected="selected">Ping (GCM only)</option>
                            <option value="ping-ios">Ping (APNS only)</option>
                            <option value="push-android">Push (GCM with payload)</option>
                            <option value="push-ios">Push (APNS with payload</option>
                            <option value="message-android">In-App Message (Android)</option>
                            <option value="message-ios">In-App Message (iOS)</option>
                        </select>
                    </div>
                    <div class="form-group">
                        <div class="input-group">
                            <input class="form-control" type="text" placeholder="your message" name="message-payload">
                            <div class="btn btn-primary invert input-group-addon" did="messaging">Send</div>
                        </div>
                    </div>
                </div>
                <div id="messaging-demo-code" class="clearfix"></div>
            </div>
            <div class="col-sm-4 text-center">
                <div id="messaging-demo-phone">
                    <div id="messaging-demo-phone-wrapper">
                        <div>my message</div>
                    </div>
                    <img src="/img/developer/messaging_demo_phone.png" class="img-responsive" />
                </div>
            </div>
        </div>
    </div>
</div>

